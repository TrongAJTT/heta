{
  "manifest_version": 3,
  "name": "Heta - Batch URL Manager",
  "version": "1.0.0",
  "description": "Batch URL generator and manager with profile support",
  "permissions": ["storage", "tabs"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.svg",
      "48": "icon48.svg",
      "128": "icon128.svg"
    }
  },
  "icons": {
    "16": "icon16.svg",
    "48": "icon48.svg",
    "128": "icon128.svg"
  }
}
