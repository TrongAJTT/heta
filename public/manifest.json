{
  "manifest_version": 3,
  "name": "Heta - Tab Helper",
  "version": "1.0.0",
  "description": "Batch URL generator and manager with profile support",
  "permissions": ["storage", "tabs", "declarativeNetRequest"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
